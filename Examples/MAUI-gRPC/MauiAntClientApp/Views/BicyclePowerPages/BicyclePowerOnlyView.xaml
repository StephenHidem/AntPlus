<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:views="clr-namespace:MauiAntClientApp.Views.BicyclePowerPages"
             xmlns:viewmodels="clr-namespace:MauiAntClientApp.ViewModels"
             x:DataType="viewmodels:BicyclePowerViewModel"
             x:Class="MauiAntClientApp.Views.BicyclePowerPages.BicyclePowerOnlyView">
    <VerticalStackLayout>
        <views:PowerView Power="{Binding BicyclePower.PowerSensor}"/>
        <views:TEPSView TEPS="{Binding BicyclePower.PowerSensor.TorqueEffectiveness}"/>
    </VerticalStackLayout>
</ContentView>
