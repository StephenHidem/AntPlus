<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiAntGrpcClient.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiAntGrpcClient"
    xmlns:pages="clr-namespace:MauiAntGrpcClient.Pages"
    Shell.FlyoutBehavior="Disabled"
    Shell.ForegroundColor="{AppThemeBinding Light={StaticResource Black}, Dark={StaticResource SecondaryDarkText}}"
    Title="MauiAntGrpcClient">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate pages:HomePage}"
        Route="HomePage" />

</Shell>
